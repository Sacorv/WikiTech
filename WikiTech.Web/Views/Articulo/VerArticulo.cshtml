@model WikiTech.Entidades.Articulo

@{
}

<h1>@Model.Titulo</h1>

<div class="card mb-3 w-75 text-center m-auto">
    <img src="@Model.ImagenPrincipal" class="card-img-top" alt="..." style="height:"450px">
    <div class="card-body">
        <h5 class="card-title">@Model.Titulo</h5>
        <p class="card-text">@Model.Contenido</p>
        <p class="card-text"><small class="text-muted">@Model.Fecha</small></p>
    </div>
</div>
